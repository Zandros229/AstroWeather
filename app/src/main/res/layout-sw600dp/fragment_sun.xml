<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:tools="http://schemas.android.com/tools"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="#FF4500"
              tools:context=".SunFragment" android:orientation="vertical">

    <TextView
            android:text=""
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/actualTimeSun"
            android:textAlignment="center"
            android:textSize="48sp"
            android:textStyle="bold"/>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp">
        <TextView
                android:id="@+id/longitudeSun"
                android:layout_width="272dp"
                android:layout_height="match_parent"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>


        <TextView
                android:id="@+id/latitudeSun"
                android:layout_width="308dp"
                android:layout_height="match_parent"
                android:text=""
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>


    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp">
        <TextView
                android:id="@+id/dlugosc_geograficznaSun"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Długość geograficzna"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="75dp"/>
        <TextView
                android:id="@+id/szerokosc_geograficznaSun"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Szczerokość geograficzna"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="75dp"/>
    </LinearLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp">
        <TextView
                android:id="@+id/sunRise"
                android:layout_width="272dp"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>

        <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/sunRaiseAzimuth"
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>

    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp">
        <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Wschód"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="75dp"/>
        <TextView
                android:text="Wschód Azymut"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="95dp"/>
    </LinearLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp">
        <TextView
                android:id="@+id/sunSet"
                android:layout_width="270dp"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>
        <TextView
                android:id="@+id/sunSetAzimuth"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>


    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp">
        <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Zachód"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="75dp"/>

        <TextView
                android:id="@+id/textView3"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Zachód Azymut"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="95dp"/>

    </LinearLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="30dp">
        <TextView
                android:id="@+id/sunCivRise"
                android:layout_width="272dp"
                android:layout_height="match_parent"
                android:text="TextView"
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>


        <TextView
                android:text="TextView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/sunCivSet"
                android:textAlignment="viewStart"
                android:textSize="24sp"
                android:textStyle="bold"
                android:layout_marginLeft="75dp"/>

    </LinearLayout>
    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="20dp">
        <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textAlignment="center"
                android:textSize="14sp"
                android:text=" Świt cywilny"
                android:layout_marginLeft="75dp"/>
        <TextView
                android:text="Zmierzch cywilny"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/textView18"
                android:textAlignment="center"
                android:textSize="14sp"
                android:layout_marginLeft="75dp"/>
    </LinearLayout>

</LinearLayout>