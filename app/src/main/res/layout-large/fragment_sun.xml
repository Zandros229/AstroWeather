<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SunFragment"
        android:background="#FF4500"
        android:orientation="vertical">

    <TextView
            android:text=""
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/actualTimeSun"
            android:textAlignment="center"
            android:textSize="50sp"
            android:textStyle="bold|italic"/>

    <TextView
            android:id="@+id/sunRise"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center"
            android:textSize="35sp"
            android:textStyle="italic"/>
    <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Rise"
            android:textAlignment="center"
            android:textSize="30sp"/>
    <TextView
            android:id="@+id/sunSet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center"
            android:textSize="35sp"
            android:textStyle="italic"/>
    <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Set"
            android:textAlignment="center"
            android:textSize="30sp"/>
    <TextView
            android:id="@+id/longitudeSun"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:textAlignment="center"
            android:textSize="35sp"
            android:textStyle="italic"/>
    <TextView
            android:id="@+id/dlugosc_geograficznaSun"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="longitude"
            android:textAlignment="center"
            android:textSize="30sp"/>

    <TextView
            android:id="@+id/latitudeSun"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""
            android:textAlignment="center"
            android:textSize="35sp"
            android:textStyle="italic"/>

    <TextView
            android:id="@+id/szerokosc_geograficznaSun"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="latitude"
            android:textAlignment="center"
            android:textSize="30sp"/>

    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/sunRaiseAzimuth" android:textAlignment="center"
            android:textSize="35sp" android:textStyle="italic"/>
    <TextView
            android:text="Rise Azimuth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView" android:textAlignment="center"
            android:textSize="30sp"/>

    <TextView
            android:id="@+id/sunSetAzimuth"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center"
            android:textSize="35sp"
            android:textStyle="italic"/>

    <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Set Azimuth"
            android:textAlignment="center"
            android:textSize="30sp"/>
    <TextView
            android:id="@+id/sunCivRise"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center"
            android:textSize="35sp"
            android:textStyle="italic"/>

    <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="center"
            android:textSize="30sp" android:text="civil dawn"/>
    <TextView
            android:text="TextView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/sunCivSet" android:textAlignment="center"
            android:textSize="35sp" android:textStyle="italic"/>
    <TextView
            android:text="civil twilight"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" android:id="@+id/textView18" android:textAlignment="center"
            android:textSize="30sp"/>

</LinearLayout>